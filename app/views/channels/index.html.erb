<p class="notice"><%= notice %></p>
<p class="alert"><%= alert %></p>
<h1 class="main_title">Join the fun!</h1>
<ul class="channels">
  <% Channel.find_each do |channel| %>
    <li><%= link_to(channel) do %><h2><%=channel.name%></h2><%= image_tag("bg-song.png", class: 'bg-background') %><%= image_tag(channel.image) %><% end %></li>
  <% end %>
</ul>